{
  "name": "news_pipeline",
  "version": "v1",
  "steps": [
    {
      "id": "step1_dedup",
      "task": "news_dedup",
      "input_from": "sample",
      "params_override": {"temperature": 0.1}
    },
    {
      "id": "step2_extract",
      "task": "ie_json",
      "input_from": "step1_dedup",
      "params_override": {"temperature": 0.0}
    },
    {
      "id": "step3_score",
      "task": "stock_score",
      "input_from": "step2_extract",
      "params_override": {"temperature": 0.2}
    }
  ]
}
